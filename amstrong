class Armstrong{
    public static void main(String[] args) {
        int n=153;
        int sum=0;
        int num=n;
        while(n>=0){
            int r=n%10;
            sum=sum+r*r*r;
            n=n/10;

        }
        if(sum==num){
            System.out.println("Armstrong number");
        }
        else{
            System.out.println("Not an armstrong number");
        }
        

    }
