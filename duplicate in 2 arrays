public class duplicate {
    public static void main(String[] args) {
        int []arr1={10,20,30};
        int []arr2={30,40};
        for(int i=0;i<=arr1.length-1;i++){
            for(int j=0;j<arr2.length-1;j++){
                if(arr1[i]==arr2[j]){
                    System.out.println(arr1[i]);

    }
    
}
